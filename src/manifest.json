{
  "manifest_version": 3,
  "name": "NatWest Login Util",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["https://*.natwest.com/*"],
      "js": ["content-script.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open login utility",
    "default_popup": "popup/popup.html"
  }
}
